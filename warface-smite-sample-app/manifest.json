{
	"manifest_version":"1",
	"type":"WebApp",
	"meta":{
		"name":"GameEventsTester",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.89.7.0",
		 "author":"Overwolf",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"GameEventsTester",
		"dock_button_title":"Game_Events_tester"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":"true",
					"grab_keyboard_focus":"true",
					"resizable":"false",
					"size":{
						"width": 800,
						"height":650
					},
					"min_size":{
						"width": 600,
						"height":600
					},
					"max_size":{
						"width": 1200,
						"height":1000
					},
					"start_position":{
						"top": 100,
						"left": 100
					},
					"show_in_taskbar":"true",
					"in_game_only":"false",
					"clickthrough":"false",
					"topmost":"true"
				}
			},
		"hotkeys":{
				"show":{
				"title":"Show/hide",
				"category":"Testers",
				"action-type":"toggle",
				"default": "Shift+F12",
				"passthrough":"false"
			}
		},
		"game_events":[8134, 9530, 7784, 5426],
		"launch_events": [
		  {
			"event": "GameLaunch",
			"event_data": {
			  "game_ids": [
				5426, 8134, 9530, 7784
			  ]
			},
			"start_minimized": false
		  }]
		
	}
}